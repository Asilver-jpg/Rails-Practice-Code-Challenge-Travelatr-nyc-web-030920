<h1>New Post</h1>

<%=form_for(@post) do |t| %>
    <%=t.label :title%>
    <%=t.text_field :title%>
    <%=t.label :content%>
    <%=t.text_field :content%>
    <%=t.label :blogger_name %>
    <%=t.collection_select :blogger_name, Blogger.all, :name, :name %>
    <%=t.label :destination_name %>
    <%=t.collection_select :destination_name, Destination.all, :name, :name %>
    <%= t.submit%>
<%end%>





 <%# t.string "title"
    t.text "content"
    t.integer "likes"
    t.integer "blogger_id"
    t.integer "destination_id" %>